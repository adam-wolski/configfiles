! Numix colors
#define N_white         #f9f9f9
#define N_gray          #dedede
#define N_orange        #d64937
#define N_darkgray      #333333
#define N_darkgrayl     #404040
#define N_black         #2d2d2d
#define N_green         #81C784

! To only apply colors to your terminal, for example, prefix
! the color assignment statement with its name. Example:
!
! URxvt*background:            S_base03

*background:              N_darkgray
*foreground:              N_white
*fading:                  40
*fadeColor:               S_base03
*cursorColor:             S_base1
*pointerColorBackground:  S_base01
*pointerColorForeground:  S_base1

